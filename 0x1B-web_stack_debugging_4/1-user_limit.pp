# Puppet Manifest to adjust system limits for the holberton user

# Explanation: Adjust system limits for the holberton user
exec { 'change-os-configuration-for-holberton-user':
  cnifest to adjust system limits for the holberton user

# Explanation: Adjust system limits for the holberton user
exec { 'change-os-configuration-for-holberton-user':
  command => '/usr/bin/sudo /usr/bin/tee -a /etc/security/limits.conf <<< "holberton soft nofile 4096" && /usr/bin/sudo /usr/bin/tee -a /etc/security/limits.conf <<< "holberton hard nofile 4096"',
}
ommand => 'echo "holberton soft nofile 4096" | sudo tee -a /etc/security/limits.conf && echo "holberton hard nofile 4096" | sudo tee -a /etc/security/limits.conf',
}

